{
    
    "manifest_version": 2,
    "name": "eTab",
    "description": "An extension that will provide you with a motivating picture whenever you open a new tab in your browser. My first Chrome extension.",
    "author": "Enej Licina",
    "homepage_url": "http://www.enejlicina.com",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "new_tab.html"
    },
    "icons": {
        "18": "icons/open_new18.png",
        "36": "icons/open_new36.png",
        "96": "icons/open_new96.png"
    },
    "permissions": ["https://source.unsplash.com/"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
    
}